#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>

int main (){

 setlocale (LC_ALL, "portuguese");


int i;
int nota ;
float media ;
int soma = 0;

for ( i = 0; i < 2; i++)
{
    do
    {
        
        printf ("Digite a nota do aluno: " , i + 1);
        scanf("%d", &nota);
    } while (nota < 1|| nota > 10);
soma += nota;
    
}

media = soma  / i;
printf("Media %.2f", media);














return 0;
}
