#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main(){
 

int x;
int i;


printf("Digite um valor: ");
scanf("%d", &x);

 for ( i = x ; i >= 0; i--)
 {
    printf("%d \n", i );
 
switch (i)
{
case 10:
    printf("FALTAM 10 NUMEROS \n");
    break;
case 20:
    printf("FALTAM 20 NUMEROS \n");
    break;
case 30:
    printf("FALTAM 30 NUMERO \n");
    break;
case 40:
    printf("FALTAM 40 NUMERO \n");
    break;
case 50:
    printf("FALTAM 50 NUMERO \n");
    break;
case 60:
    printf("FALTAM 60 NUMERO \n");
    break;
case 70:
    printf("FALTAM 70 NUMERO \n");
    break;
case 80:
    printf("FALTAM 80 NUMERO \n");
    break;
case 90:
    printf("FALTAM 90 NUMERO \n");
    break;
case 100:
    printf("FALTAM 100 NUMERO \n");
    break;
case 110:
    printf("FALTAM 110 NUMERO \n");
    break;
case 120:
    printf("FALTAM 120 NUMERO \n");
    break;
case 130:
    printf("FALTAM 130 NUMERO \n");
    break;
case 140:
    printf("FALTAM 140 NUMERO \n");
    break;
case 150:
    printf("FALTAM 150 NUMERO \n");
    break;
case 160:
    printf("FALTAM 160 NUMERO \n");
    break;
case 170:
    printf("FALTAM 170 NUMERO \n");
    break;

default:
    break;
}
 
 
 
 }


 
 
 
 










return 0;
}
