#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <locale.h>
#include <ctype.h>
int main()
{

    setlocale(LC_ALL, "portuguese");

    int i , j;

    // DE UM ATÉ 10
    // para i de 1 até 10 paso 1 faça
    // pra mudar o passo i+=2 ou i=i+2


printf("Digite um valor da tabauda: ");
scanf("%d", &j );






     for (i = 1; i <= 10; i++) {
        printf(" %d x %d = %d \n", j, i , j * i);
    }
  
    printf("\n");

   

    return 0;
}
