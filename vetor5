#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main (){

int numeros[10];
int i;
int contador = 0;
int soma = 0;

for ( i = 0; i < 10; i++)
{
    printf("\nDigite o %d numero: \n", i+1 );
    scanf("%d", &numeros[i]);

        if (numeros[i] < 0)
        {
            contador++;
        } else
        {
            soma+=numeros[i];
        }
        
        

}



   printf("\nSoma numeros positivos: %d \n",  soma); 
   printf("Quantidade numeros negativos: %d \n",  contador); 
  




return 0;
}
